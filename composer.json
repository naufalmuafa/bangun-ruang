{
    "name": "naufal/bangun-ruang",
    "description": "Library sederhana untuk menghitung bangun ruang",
    "type": "library",
    "version": "1.0.1",
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "naufal muafa",
            "email": "naufalmuafa01@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Naufal\\BangunRuang\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Naufal\\BangunRuang\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naufal\\BangunRuang\\Providers\\BangunRuangServiceProvider"
            ],
            "aliases": {
                "PersegiPanjang": "Naufal\\BangunRuang\\Facades\\PersegiPanjang"
            }
        }
    }
}
